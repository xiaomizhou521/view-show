<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root defDateFormat="yyyy-MM-dd HH:mm:ss" driver="cn.com.enorth.utility.file.impl.xml_util.XmlDriver" version="1.0">
  <CONFIG_FILENAME class="java.lang.String" comment="strutsx配置文件名"><![CDATA[strutsx_config/strutsx.xml]]></CONFIG_FILENAME>
  <UPLOAD_DEF_CONFIG class="cn.com.enorth.utility.app.web.strutsx.impl.vo.config.StrutsXUploadConfigVo" comment="文件上传的默认配置">
    <tempDir class="java.lang.String"><![CDATA[C:\Users\Administrator]]></tempDir>
    <sizeThreshold class="java.lang.Integer">102400</sizeThreshold>
    <charset class="java.lang.String"><![CDATA[UTF-8]]></charset>
    <singleFileSizeMax class="java.lang.Long">512000000</singleFileSizeMax>
    <sizeMax class="java.lang.Long">204857600000</sizeMax>
  </UPLOAD_DEF_CONFIG>
  <REQUEST_ATT_NAME_EXCEPTION class="java.lang.String" comment="发生异常，跳转至异常页面时，request.attribute中存放异常的key"><![CDATA[exception]]></REQUEST_ATT_NAME_EXCEPTION>
  <TEMPLATE_KEY class="java.lang.String" comment="strutsx配置文件中，存放action进行freemark解析的变量名"><![CDATA[action]]></TEMPLATE_KEY>
  <PARAM_NAME_MAX_LEVEL class="java.lang.Integer" comment="绑定变量名最多有几级.">8</PARAM_NAME_MAX_LEVEL>
  <NO_RULE_END_STR class="java.util.HashSet" comment="strutsx的包配置文件最后不能以什么字符结尾，目前主要会混淆 /">
    <item class="java.lang.String"><![CDATA[/]]></item>
  </NO_RULE_END_STR>
  <DEFAULT_PRINT_URL_ON class="java.lang.Boolean" comment="当log4j配置成debug状态时，strutsx启动后是否在控制台输出配置文件映射后的url">true</DEFAULT_PRINT_URL_ON>
  <CHECK_REPEAT_SUBMIT_ANNOTATION_CACHE_SIZE class="java.lang.Integer" comment="系统检查重复提交的类注释cache">1000</CHECK_REPEAT_SUBMIT_ANNOTATION_CACHE_SIZE>
</root>
